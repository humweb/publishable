{
  "name": "humweb/publishable",
  "type": "package",
  "license": "MIT",
  "authors": [
    {
      "name": "Ryan Shofner",
      "email": "ryun@humboldtweb.com"
    }
  ],
  "description": "Publishable trait.",
  "keywords": [
    "publish",
    "scope",
    "laravel"
  ],
  "require": {
    "php": ">=5.6.4",
    "laravel/framework": "5.5.*",
    "nesbot/carbon": "~1.20"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "orchestra/testbench": "~3.5",
    "orchestra/database": "~3.5.0"
  },
  "autoload": {
    "psr-4": {
      "Humweb\\Publishable\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ],
    "psr-4": {
      "Humweb\\Tests\\Publishable\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
